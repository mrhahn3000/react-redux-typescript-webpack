{
  "name": "react-redux-typescript-webpack",
  "version": "1.0.0",
  "description": "A simple react redux typescript starter project",
  "scripts": {
    "build": "webpack --config config/webpack.config.babel.js",
    "dev": "webpack-dev-server --config config/webpack.config.babel.js",
    "test": "`npm bin`/mocha --compilers js:babel-core/register --require ./app/tests/test_helper.js '*/tests/**/*.@(js|jsx)'",
    "sass": "sass --watch src/styles/scss:src/styles/css",
    "deploy": "NODE_ENV=production webpack -p --config config/webpack.config.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrhahn3000/react-redux-typescript-webpack.git"
  },
  "keywords": [
    "boilerplate",
    "redux",
    "react",
    "webpack",
    "sass",
    "scss",
    "typescript"
  ],
  "engines": {
    "node": "9.5.0",
    "npm": "5.6.0",
    "yarn": "1.3.2"
  },
  "author": "Kevin Faust",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrhahn3000/react-redux-typescript-webpack/issues"
  },
  "homepage": "https://github.com/mrhahn3000/react-redux-typescript-webpack#readme",
  "devDependencies": {
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "hocus-pocus": "^0.3.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.0",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "ts-loader": "^3.5.0",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-notifier": "^1.5.1"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
